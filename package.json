{
  "name": "jirafy-changelog",
  "version": "1.1.0",
  "description": "Generates a changelog from two references, where the markdown is formatted for any referenced Jira stories.d",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build ./index.js",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "precommit": "npm run build && git add dist/",
    "format-check": "prettier --check **/*.{js,yml,json}",
    "format": "prettier --write **/*.{js,yml,json}",
    "lint": "eslint ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coltdorsey/jirafy-changelog.git"
  },
  "keywords": [
    "jira",
    "changelog",
    "difference",
    "release notes",
    "merged",
    "pull request"
  ],
  "author": "Colt Dorsey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coltdorsey/jirafy-changelog/issues"
  },
  "homepage": "https://github.com/coltdorsey/jirafy-changelog#readme",
  "dependencies": {
    "@actions/core": "^1.5.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "jira-client": "^8.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.31.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0"
  }
}
